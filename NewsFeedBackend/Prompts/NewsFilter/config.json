{
  "name": "extract",
  "description": "Parse a free-text preference into strict JSON",
  "template_format": "handlebars",
  "input_variables": [
    { "name": "userQuery", "type": "string", "is_required": true }
  ],
  "execution_settings": {
    "default": {
      "temperature": 0.2,
      "topP": 0.9,
      "maxOutputTokens": 256,
      "response_mime_type": "application/json"
    },
    "google": {
      "temperature": 0.2,
      "topP": 0.9,
      "maxOutputTokens": 256,
      "response_mime_type": "application/json"
    }
  }
}
