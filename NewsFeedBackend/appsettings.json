{
  "GoogleAi": {
    "ChatModel": "gemini-1.5-pro",
    "EmbeddingModel": "text-embedding-004"
  },
   "NewsData": {  
    "BaseUrl": "https://newsdata.io/api/1/",
    "DefaultLanguage": "en",
    "DefaultQuery": "",     
    "DefaultCountry": "US",         
    "DefaultCategory": ""
  },
   "Jwt": {
    "Issuer": "NewsFeedBackend",
    "Audience": "NewsFeedFrontend",
    "ExpiresHours": 12
  },
  "Logging": {
  "LogLevel": {
    "Default": "Information",
    "Microsoft": "Warning",
    "System": "Warning",
    "Microsoft.AspNetCore": "Error",
    "System.Net.Http.HttpClient": "Error",
    "System.Net.Http.HttpClient.newsdata": "Error"
  },
  "Console": {
    "FormatterName": "simple",
    "FormatterOptions": { "SingleLine": true, "TimestampFormat": "HH:mm:ss " }
  }
},
  "AllowedHosts": "*"
}
